<UserControl x:Class="Alexus.ThinMvvm.Client.TestCases.TestCase1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:testCases="clr-namespace:Alexus.ThinMvvm.Client.TestCases"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
			 d:DataContext="{d:DesignInstance testCases:TestCase1ViewModel }"
			 >
    <StackPanel Background="LightSkyBlue">
        <TextBlock Text="{Binding Path=Model.Id}" Background="LightGray"/>
		<TextBlock Text="{Binding Path=Model.Data}" Background="LightBlue"/>
    </StackPanel>
</UserControl>
